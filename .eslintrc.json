{
    "env": {
        "browser": true,
        "es6": true,
        "amd": true,
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": "off"
    },
    "settings": {
        "import/resolver": "webpack"
      }
}